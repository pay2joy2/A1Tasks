CREATE TABLE IF NOT EXISTS public.postings
(
    "Mat. Doc." character varying(30) COLLATE pg_catalog."default" NOT NULL,
    item integer NOT NULL,
    "Doc. Date" date NOT NULL,
    "Pstng Date" date NOT NULL,
    "Material Description" character varying(100) COLLATE pg_catalog."default" NOT NULL,
    quantity integer NOT NULL,
    bun character varying(30) COLLATE pg_catalog."default" NOT NULL,
    "Amount LC" double precision NOT NULL,
    crcy character varying(10) COLLATE pg_catalog."default" NOT NULL,
    "User Name" character varying(30) COLLATE pg_catalog."default" NOT NULL,
    "Authorized posting" boolean NOT NULL
)
